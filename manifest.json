{
  "manifest_version": 3,
  "name": "Canvas Quiz Helper",
  "version": "1.0",
  "description": "Helps you with Canvas quizzes by showing questions and answers in a side panel",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.instructure.com/*",
        "*://webcourses.ucf.edu/*"
      ],
      "js": ["dist/content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 