{
  "name": "canvas-assistant-bot",
  "version": "1.0.0",
  "description": "An automated tool for answering Canvas LMS quizzes using browser automation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "keywords": [
    "canvas",
    "lms",
    "automation",
    "puppeteer",
    "typescript"
  ],
  "author": "Brett",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.5",
    "puppeteer": "^22.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "directories": {
    "test": "tests"
  },
  "type": "commonjs"
}
